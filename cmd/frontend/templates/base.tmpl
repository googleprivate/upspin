<style>
*,
:before,
:after {
  box-sizing: border-box;
}
html,
body {
  height: 100%;
}
body {
  display: flex;
  flex-direction: column;
  font: 14px arial, sans-serif;
  line-height: 1.45;
  margin: 0;
  padding: 0;
}
footer {
  background-color: #f1f2f3;
  padding: 1em;
}
main {
  flex: 1;
  padding: 0 1em;
}
.container {
  margin: 0 auto;
  max-width: 50em;
}
.homeLink:link,
.homeLink:visited {
  color: #000;
  text-decoration: none;
}
